{
  "deploy": {
    "startCommand": "python main.py",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 180,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 2
  },
  "build": {
    "builder": "DOCKERFILE"
  },
  "volumes": [
    {
      "name": "data",
      "mountPath": "/app/data"
    }
  ]
}
